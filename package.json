{
  "name": "modus-web-components-example",
  "version": "1.0.0",
  "private": "true",
  "description": "modus web components examples",
  "homepage": "https://coliff.github.io/modus-web-components-example/",
  "bugs": {
    "url": "https://github.com/coliff/modus-web-components-example/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coliff/modus-web-components-example.git"
  },
  "license": "MIT",
  "scripts": {
    "htmlhint": "htmlhint **/*.html",
    "prettier": "npx prettier --write \"**/*.*s\"",
    "pa11y": "npx pa11y https://coliff.github.io/modus-web-components-example/ --hide-elements \".pa11y-ignore\"",
    "pa11y-ci": "npx pa11y-ci",
    "spellcheck": "npx cspell \"**/*.*\" --no-progress",
    "start": "npx lite-server -c bs-config.json"
  },
  "dependencies": {
    "@trimble-oss/modus-web-components": "1.2.0",
    "htmlhint": "1.6.3",
    "hugo-bin": "0.144.9",
    "popover-css-inspector": "1.0.0-beta16"
  },
  "engines": {
    "node": ">=22"
  },
  "volta": {
    "node": "22.17.0"
  }
}
